    "#equivalence sequence"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def geometric(n):\n",
    "    result=[]\n",
    "    a=1\n",
    "    while True:\n",
    "        r=a*a\n",
    "        if r>n:\n",
    "            break\n",
    "        result.append(r)\n",
    "        a=a+1\n",
    "    return r"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "print(geometric(16))"
   ]
  },
